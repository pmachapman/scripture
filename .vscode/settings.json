{
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.formatOnSave": true,
  "editor.rulers": [100],
  "editor.wordWrapColumn": 100,

  "files.eol": "\n",

  "json.schemas": [
    {
      "fileMatch": ["*tsconfig*.json"],
      "url": "http://json.schemastore.org/tsconfig"
    }
  ],
  "cSpell.words": ["Parseable"]
}
